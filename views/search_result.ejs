<script type="text/javascript" src="/javascripts/jquery-ui.js"></script>
<script>
$.getJSON('//ipinfo.io/json', function(data) {
	  window.ip = data.ip;
	});

	function getJsonFromUrl() {
		var query = location.search.substr(1);
		var result = {};
		query.split("&").forEach(function(part) {
			var item = part.split("=");
			result[item[0]] = decodeURIComponent(item[1]);
		});
		return result;
	}

	function applyNowClick (product) {
		var dt;
		if ((navigator.userAgent.indexOf("MSIE") != -1 ) || (!!document.documentMode == true )) {
			dt = (new Date()).toLocaleString().replace(" ", "T")
		} else {
			dt = (new Date).toLocaleDateString().split(".").reverse().toString().replace(/,/g, "-") + "T" + (new Date).toLocaleTimeString()
		}
		var url = getJsonFromUrl();
		dataLayer.push({
		event: 'checkout',
			ecommerce: {
				purchase: {
					actionField: { 
						step: 1, 
						id: window.ip + '_' + dt,
						list: url.from,
					},
					products: [{
						name: product.name + ' ' + product.provider,
						id: product.id,
						price: 0,
						brand: product.provider,
						category: product.category + '/' + product.subcategory,
						variant: '',
						quantity: 1
					}]
				},
				checkout: {
					actionField: { 
						step: 1, 
						list: url.from,
					},
					products: [{
						name: product.name + ' ' + product.provider,
						id: product.id,
						price: 0,
						brand: product.provider,
						category: product.category + '/' + product.subcategory,
						variant: '',
						quantity: 1
					}]
				}
			}
		});
	}
	function readMoreClick (product) {
		var url = getJsonFromUrl();
		dataLayer.push({
		event: 'productClick',
		ecommerce: {
			click: {
				actionField: { list: url.from },
				products: [{
					name: product.name + ' ' + product.provider,
					id: product.id,
					price: 0,
					brand: product.provider,
					category: product.category + '/' + product.subcategory,
					variant: '',
					position: 1
				 }]
				}
			}
		});
	}
</script>
<div class="dimness"></div>
	<header><% include partials/templemate/header.ejs %></header>
	<div class="wrapper">
		<article class="search-result">
			<h1> <%= productList[0].subcategory %></h1>
			<div class="products">
				<% productList.forEach(product => { %>
					<% if (product.alive) { %>
					<div class="single-product">
						<div class="img-wrapper">
							<img src ="<%=product.logo%>">
						</div>
						<div class="desc">
							<h2><%=product.name%> <%=product.provider%></h2>
							<div class="info">
								<div class="inner-scroll">
									<%-product.description%>
								</div>
							</div>
						</div>
						<div class="buttons-container">
							<% if (product.application.isActive) { %>
							<a class="button apply-now" onclick="applyNowClick(<%=JSON.stringify(product)%>)" href="<%=product.application.redirect%>" rel="nofollow" target="_blank">Zloż wniosek</a>
							<% } %>
							<a class="button read-more" onclick="readMoreClick(<%=JSON.stringify(product)%>)" href="/finanse/produkt/<%=product.value%>?from=Product_details">Dowiedz się więcej</a>
						</div>
					</div>
					<%}%>
				<%})%>
			</div>
		</article>
	</div>
	<footer><% include partials/templemate/footer.ejs %></footer>
	<script src="/javascripts/typeahead.js"> </script>
	<script src="/javascripts/main.js"></script>
	<script>
	  function initMap() {
		var uluru = {lat: 52.235765, lng: 21.0177286};
		var map = new google.maps.Map(document.getElementById('map'), {
		  zoom: 13,
		  center: uluru,
		  disableDefaultUI: true,
		  zoomControl: true
		});
		var marker = new google.maps.Marker({
		  position: uluru,
		  map: map
		});
	  }

	</script>
	<script async defer
	src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCMfnryrSqfNlO6vbG6RgbyZDobT7K7Ips&callback=initMap">
	</script>
	<script>
		var dataLayerObject = <%- dataLayerObject %>;
		dataLayer.push(dataLayerObject);
		dataLayer.push({
				google_tag_params: {
				dynx_items: dataLayerObject.ecommerce.impressions.map(products => products.id),
				dynx_pagetype: 'searchresults',
				dynx_totalvalue: 0
			}
		})
	</script>
</body>

</html>

    <div class="wrapper">
        <div class="dimness"></div>
        <article class="search-result">
            <div class="products">
                <div class="single-product">
                    <div class="product-header">
                        <div class="img-wrapper">
                            <img src ="<%=product.logo%>">
                        </div>
                        <h4><%=product.name%> <%=product.provider%></h4>
                    </div>
                     <p class="info">
                        <%-product.longDescription%>
                    </p>
                    <div class="show-product">
                        <h4><%=product.name%> <%=product.provider%></h4>
                        <button class="apply-now" onclick='window.location="<%=product.application%>"'>Zloż wniosek</button>
                    </div>
                </div>
                <% suggestedProducts.forEach(product => { %>
                <div class="single-product">
                    <div class="product-header">
                        <div class="img-wrapper">
                            <img src ="<%=product.logo%>">
                        </div>
                        <h4><%=product.name%> <%=product.provider%></h4>
                    </div>
                     <p class="info">
                        <%-product.description%>
                    </p>
                    <div class="show-product">
                        <h4><%=product.name%> <%=product.provider%></h4>
                        <button class="read-more" onclick='window.location="/finanse/produkt/<%=product.value%>"'>Dowiedz się więcej</button>
                        <button class="apply-now" onclick='window.location="<%=product.application%>"'>Zloż wniosek</button>
                    </div>
                </div>
                <% }) %>
            </div>
        </article>
        <aside class="aside aside-1"></aside>
        <aside class="aside aside-2"></aside>
    </div>

    <footer> <% include partials/templemate/footer.ejs %> </footer>

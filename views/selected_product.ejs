            <ul class="dl-menu">
                <% categoriesDictionary.forEach( category => { %>
                    <li class="category">
                    <a href="#"><%=category%></a>
                    <% categoriesMap[category].forEach( subcategory => { %>
                        <ul class="dl-submenu">
                            <li>
                                <a href="/finanse/<%=subcategory.replace(/ /g, '-')%>"><%=subcategory%></a>
                            </li>
                        </ul>

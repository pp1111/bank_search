	<script src="/javascripts/main.js"></script>
	<script src="/javascripts/red.js"></script>
	<script>
		$(document).on("ready", function(){
			$('html,body').animate({ scrollTop: 580 }, { duration: 1500 });
		});
    </script>
	<script>
      function initMap() {
        var uluru = {lat: 52.235765, lng: 21.0177286};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 13,
          center: uluru,
          disableDefaultUI: true,
          zoomControl: true
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCMfnryrSqfNlO6vbG6RgbyZDobT7K7Ips&callback=initMap">
    </script>

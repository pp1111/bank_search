	<div class="logo-container">
		<a href="/"><img class="logo" src="/images/logo.png" alt="aMoney"></a>
	</div>
	<div class="wrapper">
		<header>
		<h1>Kalkulator Walut</h1>
		<form id="calc">
			<div class="value">
				<% include partials/content/text_input.ejs %>
			</div>
			<div class="values">
				<div class ="course">
					<% include partials/content/course.ejs %>
				</div>
				<div class="date">
					<% include partials/content/date.ejs %>
				</div>
			</div>
		</form>	
		</header>
		<% include partials/content/result.ejs %>
		
	</div>
	<footer><% include partials/templemate/footer.ejs %></footer>

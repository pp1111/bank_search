<div class="dl-menu-header">
    <!--<h3>menu</h3>-->
    <div class="menu-btn">
        <div class="bar top-bar"></div>
        <div class="bar middle-bar"></div>
        <div class="bar bottom-bar"></div>
    </div>
</div>
<div class="dl-menu-container">
    <a href="/kontakt">Kontakt</a>
    <div class="inner-scroll">
    <ul class="dl-menu">
        <div class="inner-scroll">
            <li><a class="calc-link" href="/przelicznik">Kalkulator walut</a></li>
            <% categoriesDictionary.forEach( category => { %> 	 	
                <li class="category">
                <a href="#" class="category-link"><%=category%></a>
                    <ul class="dl-submenu">
                        <div class="inner-scroll">
                            <% categoriesMap[category].forEach( subcategory => { %>
                            <li class="subcategory">
                                <a class="subcategory-link" href="/finanse/<%=subcategory.replace(/ /g, '-')%>"><%=subcategory%></a>
                            </li>
                            <% }) %>
                        </div>
                    </ul>
            <% }) %>
        </div>
    </ul>
    <div class="line"></div>
    </div>
</div>